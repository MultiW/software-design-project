package employeecreator;

import warehousing.Employee;
import warehousing.Sequencer;

public class SequencerCreator extends EmployeeCreator {

  /**
   * Constructor for SequencerCreator.
   * 
   */
  public SequencerCreator() {}

  /**
   * create a Sequencer with input username.
   */
  @Override
  public Employee createEmployee(String username) {
    return new Sequencer(username);
  }
}
