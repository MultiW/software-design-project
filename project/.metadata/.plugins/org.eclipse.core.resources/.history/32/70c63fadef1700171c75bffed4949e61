package employeecreator;

import warehousing.Employee;
import warehousing.Loader;

public class LoaderCreator extends EmployeeCreator {
  /**
   * Constructor for LoaderCreator.
   * 
   */
  public LoaderCreator() {}

  /**
   * create a loader with input username.
   */
  @Override
  public Employee createEmployee(String username) {
    return new Loader(username);
  }
}
